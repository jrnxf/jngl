{
  "name": "jngl",
  "colors": {
    // #0b1b1d darkest green
    // #0d1f21 darker green
    // #0e2225 mid green
    // #102628 lighter green
    "activityBar.background": "#0d1f21",
    "activityBar.border": "#1d3337",
    "badge.background": "#4d7d90",
    "dropdown.background": "#102629",
    "editor.background": "#0d1f21",
    "editor.lineHighlightBackground": "#1d3337",
    "editor.selectionBackground": "#293e40",
    "editorGroupHeader.tabsBackground": "#102629",
    "gitDecoration.addedResourceForeground": "#7aa4a1",
    "gitDecoration.deletedResourceForeground": "#e85c51",
    "gitDecoration.modifiedResourceForeground": "#fda47f",
    "gitDecoration.untrackedResourceForeground": "#5a93aa",
    "input.background": "#0b1b1d",
    "list.activeSelectionBackground": "#1d3337",
    "list.errorForeground": "#d93125",
    "list.hoverBackground": "#1d3337",
    "list.inactiveFocusBackground": "#1d3337",
    "list.inactiveSelectionBackground": "#1d3337",
    "list.warningForeground": "#d35f2d",
    "panel.border": "#1d3337",
    "quickInput.background": "#0b1b1d",
    "quickInputList.focusBackground": "#0b1b1d",
    "sash.hoverBorder": "#1d3337",
    "scrollbarSlider.background": "#1d333780",
    "sideBar.background": "#0d1f21",
    "sideBar.border": "#1d3337",
    "sideBarSectionHeader.background": "#0d1f21",
    "sideBarSectionHeader.border": "#00000000",
    "statusBar.background": "#102629",
    "tab.activeBackground": "#0d1f21",
    "tab.border": "#102629",
    "tab.hoverBackground": "#0d1f21",
    "tab.inactiveBackground": "#0e2225",
    "terminal.ansiBlack": "#2f3239",
    "terminal.ansiBlue": "#5a93aa",
    "terminal.ansiBrightBlack": "#4e5157",
    "terminal.ansiBrightBlue": "#73a3b7",
    "terminal.ansiBrightCyan": "#afd4de",
    "terminal.ansiBrightGreen": "#8eb2af",
    "terminal.ansiBrightMagenta": "#b97490",
    "terminal.ansiBrightRed": "#eb746b",
    "terminal.ansiBrightWhite": "#eeeeee",
    "terminal.ansiBrightYellow": "#fdb292",
    "terminal.ansiCyan": "#a1cdd8",
    "terminal.ansiGreen": "#7aa4a1",
    "terminal.ansiMagenta": "#ad5c7c",
    "terminal.ansiRed": "#e85c51",
    "terminal.ansiWhite": "#ebebeb",
    "terminal.ansiYellow": "#fda47f",
    "terminal.background": "#0d1f21",
    "terminal.foreground": "#e6eaea",
    "terminalCursor.background": "#102629",
    "terminalCursor.foreground": "#e6eaea",
    "titleBar.activeBackground": "#0d1f21",
    "titleBar.inactiveBackground": "#0d1f21",
    "toolbar.hoverBackground": "#1d3337"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6d7f8b"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#ebebeb"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#afd4de"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control.flow" // return
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7aa4a1"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ebebeb"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#73a3b7"
      }
    },
    {
      "name": "keyword",
      "scope": ["keyword.operator.ternary", "keyword.operator.new", "new.expr"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block", "variable.other", "meta.brace"],
      "settings": {
        "foreground": "#ebebeb"
      }
    },
    {
      "name": "Imports",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#d38d97"
      }
    },
    {
      "name": "Objects",
      "scope": [
        "variable.other.object.property",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#5a93aa"
      }
    },
    {
      "name": "keyword",
      "scope": ["keyword.operator", "storage.type.function.arrow"],
      "settings": {
        "foreground": "#cbd9d8"
      }
    },
    {
      "name": "undefined",
      "scope": ["constant.language.undefined"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e85c51"
      }
    },
    {
      "name": "null",
      "scope": ["constant.language.null"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff9664"
      }
    },
    {
      "name": "Meta",
      "scope": ["meta"],
      "settings": {
        "foreground": "#5a93aa"
      }
    },
    {
      "name": "Meta",
      "scope": ["keyword.operator.assignment", "punctuation"],
      "settings": {
        "foreground": "#cbd9d8"
      }
    },
    {
      "name": "Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#7AA4A1"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#ebebeb"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ff8349"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#7aa4a1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fda47f"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#a1cdd8"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#73a3b7"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#73a3b7"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fda47f"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#fda47f"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#73a3b7"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7aa4a1"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ad5c7c"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#7aa4a1"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#7aa4a1"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#73a3b7"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
